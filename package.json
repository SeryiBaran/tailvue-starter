    "@iconify/json": "^2.2.46",
    "@vueuse/core": "^9.13.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@iconify/tailwind": "^0.1.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.1",
    "taze": "^0.9.1",
